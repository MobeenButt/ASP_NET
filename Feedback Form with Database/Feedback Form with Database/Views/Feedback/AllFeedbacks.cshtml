@model List<Feedback_Form_with_Database.Models.Feedback>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table border="1">
    <thead>
        <tr>
            <th>Student Name</th>
            <th>Course</th>
            <th>Comments</th>
            <th>Rating</th>
            <th>Date Submitted</th>
        </tr>

    </thead>
    <tbody>
        @foreach(Feedback_Form_with_Database.Models.Feedback feedback in Model)
        {
            <tr>
                <td>@feedback.StudentName</td>
                <td>@feedback.Course</td>
                <td>@feedback.Comments</td>
                <td>@feedback.Rating</td>
                <td>@feedback.DateSubmitted.ToString("g")</td>
            </tr>
        }
    </tbody>
</table>